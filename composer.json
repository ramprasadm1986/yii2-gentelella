{
  "name": "ramprasadm1986/yii2-gentelella",
  "description": "Free admin template for backend",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","backend","bootsprap4","admin","gentelella","theme","asset"],
  "license": "Apache-2.0",
  "version": "1.0.1",
  "minimum-stability":"stable",
  "authors": [
        {
            "name": "Ramprasad Moharana",
            "email": "ramprasad@krititech.in"
        }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap4": "^2.0.0",
    "rmrevin/yii2-fontawesome": "~3.0",
    "bower-asset/gentelella": "*"
  },
  "autoload": {
    "psr-4": {
      "ramprasadm1986\\gentelella\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
